<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<div id="successNotificationActionCalendar" class="modal fade"
	data-backdrop="static" role="dialog">
	<div class="container-fluid">
		<div class="success-information-calendar-modal">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			<p class="modal-title">
				<span class="glyphicon glyphicon-info-sign"></span>Information
			</p>
			<div class="row modal-body">
				<p>Successful manipulation</p>
			</div>
		</div>
	</div>
</div>
<div id="failNotificationActionCalendar" class="modal fade"
	data-backdrop="static" role="dialog">
	<div class="container-fluid">
		<div class="fail-information-calendar-modal">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			<p class="modal-title">
				<span class="glyphicon glyphicon-info-sign"></span>Information
			</p>
			<div class="row modal-body">
				<p>Failure manipulation</p>
			</div>
		</div>
	</div>
</div>